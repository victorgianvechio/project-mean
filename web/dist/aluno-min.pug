<!DOCTYPE html><html><head><meta charset="UTF-8"><meta name="viewport" content="width=device-width, initial-scale=1.0"><script src="js/lib/angular.min.js"></script><script src="js/aluno/aluno-controller.js"></script><link rel="stylesheet" href="css/bootstrap.min.css"><link rel="stylesheet" href="css/aluno/aluno.css"><link rel="stylesheet" href="css/aluno/aluno-normalize.css"><title>Alunos</title></head><body><div ng-app="app"><h1>Aluno</h1><div ng-controller="aluno-controller"><table class="table table-bordered table-hover table-aluno"><tr><th>ID</th><th>Nome</th><th>RA</th><th>CPF</th><th>E-mail</th><th>Data Nascimento</th></tr><tr ng-repeat="aluno in alunos"><td>{{aluno._id}}</td><td>{{aluno.nome}}</td><td>{{aluno.ra}}</td><td>{{aluno.cpf}}</td><td>{{aluno.email}}</td><td>{{aluno.dtaNasc}}</td></tr></table></div></div></body></html>
